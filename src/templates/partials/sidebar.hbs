{{#if allDates}}
    <div class="sidebar">
        <h4>Archives</h4>
        <ul>
            {{#each allDates}}
                <li>
                    <a href="{{dateLink}}">{{dateStr}}</a>
                </li>
            {{/each}}
        </ul>
    </div>
{{/if}}
{{#if allTags}}
    <div class="sidebar">
        <h4>Tags</h4>
        <ul>
            {{#each allTags}}
                <li>
                    <a href="{{tagLink}}">{{tag}}</a>
                </li>
            {{/each}}
        </ul>
    </div>
{{/if}}
